    <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E55_Type"/>
    <rdfs:comment xml:lang="en">This property is used to indicate the type which is indicated as holding for the subject of the classification status.

 
</rdfs:comment>
    <rdfs:label xml:lang="en">ascribes classification</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ZP12</skos:notation>
    <rdfs:domain rdf:resource="https://ontology.swissartresearch.net/aaao/ZE4_Classificatory_Status"/>
    <rdfs:subPropertyOf rdf:resource="https://ontology.swissartresearch.net/aaao/ZP2_ascribes_intentional_target"/>
  </rdf:Property>
