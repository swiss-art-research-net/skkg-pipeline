    <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E55_Type"/>
    <rdfs:comment xml:lang="en">This property is used to indicate the specific manner in which the type which is indicated as holding for the subject of the classification status pertains to this subject.

 
</rdfs:comment>
    <rdfs:label xml:lang="en">ascribes classification relation</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ZP13</skos:notation>
    <rdfs:domain rdf:resource="https://ontology.swissartresearch.net/aaao/ZE4_Classificatory_Status"/>
    <rdfs:subPropertyOf rdf:resource="https://ontology.swissartresearch.net/aaao/ZP3_ascribes_intentional_relation"/>
